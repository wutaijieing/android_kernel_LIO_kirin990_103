menuconfig USB_AUDIO_DTSI_PROPERTY
	bool "USB audio dtsi property drivers support"
	depends on SND_USB_AUDIO
	default n
	help
	Say yes here to support the USB audio dtsi property module

config USB_AUDIO_DSP
	bool "USB audio dsp drivers support"
	depends on SND_USB_AUDIO
	default y
	help
	Say yes here to support the USB audio dsp module
