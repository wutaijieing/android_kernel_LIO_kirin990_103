config SND_SOC_PCM_CODECS
	tristate "SoC Audio support for pcm codec"
	default n

config SND_SOC_CDC_CUSTOM
	tristate "Soc Audio support for cdc custom"
	depends on SND_SOC_PCM_CODECS
	default n

config SND_SOC_ARMPC_CUSTOM
	tristate "Soc Audio support for armpc custom"
	depends on SND_SOC_PCM_CODECS
	default n

