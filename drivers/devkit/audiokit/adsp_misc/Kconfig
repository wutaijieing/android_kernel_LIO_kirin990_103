config HUAWEI_ADSP_MISC
    bool "audio dsp driver for ipi adapter"
    default n
    help
    Say Y to enable adsp misc driver support

config ADSP_HUAWEI_SHARED_MEM
    bool "mtk shared mem between ap and adsp for huawei params"
    depends on HUAWEI_ADSP_MISC
    default n
    help
    Say Y to enable shared mem driver support
